package com.enovell.yunwei.taskManage.dto;

import java.io.Serializable;
import java.util.Date;

import com.enovell.system.common.domain.User;


/**      
 * 项目名称：RINMS2MAIN
 * 类名称：TaskManageDTO   
 * 类描述:  
 * 创建人：ltf 
 * 创建时间：2016-12-13 下午3:11:34
 * 修改人：ltf
 * 修改时间：2016-12-13 下午3:11:34   
 *    
 */

public class TaskSearchDto implements Serializable {
	
	private static final long serialVersionUID = -554877800888072260L;
	
	
	/**
	 * 标题
	 */
	private String title;
	
	/**
	 * 任务类别
	 */
	private Integer taskType;
	
	/**
	 * 紧急程度
	 */
	private Integer taskLevel;
	
	/**
	 * 派发开始时间
	 */
	private Date sendStartDate;
	
	/**
	 * 派发结束时间
	 */
	private Date sendEndDate;
	
	/**
	 * 任务所属单位id（派发单位）
	 */
	private String orgId;
	
	/**
	 * 任务所属人
	 */
	private User owner;
	
	/**
	 * 抄送人
	 */
	private User copyUser;
	
	/**
	 * 创建人
	 */
	private User creatUser;
	
	/**
	 * 状态
	 */
	private String status;
	
	/**
	 * 是否是根
	 */
	private String isRoot;
	
	/**
	 * 是否超时
	 */
	private String overtimeStatus;
	
	/**
	 * 受令人单位ID(用于车间主任待办)
	 */
	private String userOrgId;

	public String getTitle() {
		return title;
	}

	public void setTitle(String title) {
		this.title = title;
	}

	public Integer getTaskType() {
		return taskType;
	}

	public void setTaskType(Integer taskType) {
		this.taskType = taskType;
	}

	public Integer getTaskLevel() {
		return taskLevel;
	}

	public void setTaskLevel(Integer taskLevel) {
		this.taskLevel = taskLevel;
	}

	public Date getSendStartDate() {
		return sendStartDate;
	}

	public void setSendStartDate(Date sendStartDate) {
		this.sendStartDate = sendStartDate;
	}

	public Date getSendEndDate() {
		return sendEndDate;
	}

	public void setSendEndDate(Date sendEndDate) {
		this.sendEndDate = sendEndDate;
	}

	public String getOrgId() {
		return orgId;
	}

	public void setOrgId(String orgId) {
		this.orgId = orgId;
	}

	public User getOwner() {
		return owner;
	}

	public void setOwner(User owner) {
		this.owner = owner;
	}

	public User getCopyUser() {
		return copyUser;
	}

	public void setCopyUser(User copyUser) {
		this.copyUser = copyUser;
	}

	public User getCreatUser() {
		return creatUser;
	}

	public void setCreatUser(User creatUser) {
		this.creatUser = creatUser;
	}

	public String getStatus() {
		return status;
	}

	public void setStatus(String status) {
		this.status = status;
	}

	public String getIsRoot() {
		return isRoot;
	}

	public void setIsRoot(String isRoot) {
		this.isRoot = isRoot;
	}

	

	public String getUserOrgId() {
		return userOrgId;
	}

	public void setUserOrgId(String userOrgId) {
		this.userOrgId = userOrgId;
	}

	public String getOvertimeStatus() {
		return overtimeStatus;
	}

	public void setOvertimeStatus(String overtimeStatus) {
		this.overtimeStatus = overtimeStatus;
	}

	@Override
	public String toString() {
		return "TaskSearchDto [title=" + title + ", taskType=" + taskType + ", taskLevel=" + taskLevel
				+ ", sendStartDate=" + sendStartDate + ", sendEndDate=" + sendEndDate + ", orgId=" + orgId + ", owner="
				+ owner + ", copyUser=" + copyUser + ", creatUser=" + creatUser + ", status=" + status + ", isRoot="
				+ isRoot + ", overtimeStatus=" + overtimeStatus + ", userOrgId=" + userOrgId + "]";
	}
	
	
}




