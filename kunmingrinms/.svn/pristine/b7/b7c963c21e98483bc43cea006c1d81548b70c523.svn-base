@CHARSET "UTF-8";
/*
* 闪烁效果
*/
@keyframes blink{
  0% { opacity: 1; }
  50% { opacity: 1; }
  50.01% { opacity: 0; }
  100% { opacity: 0; }
}
@-webkit-keyframes blink{
  0% { opacity: 1; }
  50% { opacity: 1; }
  50.01% { opacity: 0; }
  100% { opacity: 0; }
}
.yunWeiBlink{
	font-weight:bold;
	cursor:pointer;
    color: #FF8B00;
    animation: blink 1s linear infinite;  
}
	
