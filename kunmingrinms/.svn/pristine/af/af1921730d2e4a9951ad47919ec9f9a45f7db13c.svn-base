$(document).ready(function(){
	// 打开一级菜单（第三个生产管理）
//	$(".firstFuncButton :eq(2)").click();
	if(menuId==null||menuId==""){
		$(".firstFuncButton:contains('生产管理')").click();
		$(".subNav:contains('日交班信息管理')").click();
		setTimeout(openIndex,1*1000);
	}else if(menuId=="safeManage"){
		$(".firstFuncButton:contains('安全管理')").click();
		$(".subNav :eq(0)").click();
	}else if(menuId=="produceManage"){
		$(".firstFuncButton:contains('生产管理')").click();
		$(".subNav :eq(0)").click();
	}else if(menuId=="constructManage"){
		$(".firstFuncButton:contains('施工管理')").click();
		$(".subNav :eq(0)").click();
	}else if(menuId=="technologyManage"){
		$(".firstFuncButton:contains('技术管理')").click();
		$(".subNav :eq(0)").click();
	}else if(menuId=="integrateManage"){
		$(".firstFuncButton:contains('综合管理')").click();
		$(".subNav :eq(0)").click();
	}else if(menuId=="machineInfoManage"){
		$(".firstFuncButton:contains('机房信息管理')").click();
		$(".subNav :eq(0)").click();
	}
});

function openIndex() {
	$('.leftFuction :contains("值班管理")').click();
}