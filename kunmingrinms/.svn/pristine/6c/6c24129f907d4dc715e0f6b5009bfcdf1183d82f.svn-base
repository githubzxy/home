.content_right {
	width: 85%;
	height: 650px;
	float: left;
	background-color:#FFFFFF;
}

.page_content {
	border-left: 1px solid #95B8E7;
	border-bottom: 1px solid #95B8E7;
	border-right: 1px solid #95B8E7;
	width:100%;
	height:100%;
	border:0 solid #ffffff;
	overflow-x: hidden;
	overflow-y:auto;
	background-color:#FFFFFF;
	
}

.index_menu {
	overflow:auto;
	text-align:center;
	-moz-user-select:none;
	-webkit-user-select:none;
}

.hover{
	background:#cedbef;
}