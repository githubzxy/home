 *
 */

public class RoleConfigShowDTO implements Serializable{	
	
	
	private static final long serialVersionUID = -1189231963408221185L;

	/**
	 * userID
	 */
	private String userId;
	
	/**
	 * user名称
	 */
	private String userName;
	
	/**
	 * user手机联系电话
	 */
	private String userPhone;
	
	/**
	 * 是否设置了角色
	 */
	private Boolean hasOnRole;

	public String getUserId() {
		return userId;
	}

	public void setUserId(String userId) {
		this.userId = userId;
	}

	public String getUserName() {
		return userName;
	}

	public void setUserName(String userName) {
		this.userName = userName;
	}

	public String getUserPhone() {
		return userPhone;
	}

	public void setUserPhone(String userPhone) {
		this.userPhone = userPhone;
	}

	public Boolean getHasOnRole() {
		return hasOnRole;
	}

	public void setHasOnRole(Boolean hasOnRole) {
		this.hasOnRole = hasOnRole;
	}

}
