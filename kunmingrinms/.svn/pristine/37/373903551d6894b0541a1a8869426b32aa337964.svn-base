package com.enovell.yunwei.message.dto;

import java.io.Serializable;
import java.util.Date;
import java.util.List;

import com.enovell.yunwei.attach.dto.UploadFileDto;
/**
 * 
 * 项目名称：RINMS2MAIN
 * 类名称：IssueMessageDTO   
 * 类描述:  信息发布DTO
 * 创建人：lidt 
 * 创建时间：2016-11-25 下午2:08:11
 * 修改人：lidt 
 * 修改时间：2016-11-25 下午2:08:11   
 *
 */
public class IssueMessageDTO implements Serializable {
	private static final long serialVersionUID = 6440759051098803657L;
	
	private String id;

	/**
	 * 开始时间（检查时间）
	 */
	private Date startTime;

	/**
	 * 结束时间
	 */
	private Date endTime;

	/**
	 * 是否发布
	 */
	private int issue;

	/**
	 * 主题
	 */
	private String theme;
	
	/**
	 * 内容
	 */
	private String content;
	
	/**
	 * 发布时间
	 */
	private Date issueTime;
	
	/**
	 * 发布人
	 */
	private String issueUserName;
	/**
	 * 发布部门
	 */
	private String issueOrgName;
	/**
	 * 组织类型
	 */
	private Integer orgType;
	/**
	 * 组织id
	 */
	private String orgId;
	
	/**
	 * 信息发布类型ID
	 */
	private Integer type;
	
	/**
	 * 信息发布类型名
	 */
	private String typeName;
	
	/**
	 * 附件id
	 */
	private String attachFileId;
	
	/**
	 * 上传文件
	 */
	private List<UploadFileDto> uploadFileArr;
	/**
	 * 置顶状态
	 */
	private String topStatus;
	
	public Date getStartTime() {
		return startTime;
	}

	public void setStartTime(Date startTime) {
		this.startTime = startTime;
	}

	public Date getEndTime() {
		return endTime;
	}

	public void setEndTime(Date endTime) {
		this.endTime = endTime;
	}

	public int getIssue() {
		return issue;
	}

	public void setIssue(int issue) {
		this.issue = issue;
	}

	public String getTheme() {
		return theme;
	}

	public void setTheme(String theme) {
		this.theme = theme;
	}

	public String getContent() {
		return content;
	}

	public void setContent(String content) {
		this.content = content;
	}

	public Date getIssueTime() {
		return issueTime;
	}

	public void setIssueTime(Date issueTime) {
		this.issueTime = issueTime;
	}

	public String getIssueUserName() {
		return issueUserName;
	}

	public void setIssueUserName(String issueUserName) {
		this.issueUserName = issueUserName;
	}

	public Integer getType() {
		return type;
	}

	public void setType(Integer type) {
		this.type = type;
	}

	public String getId() {
		return id;
	}

	public void setId(String id) {
		this.id = id;
	}

	public String getTypeName() {
		return typeName;
	}

	public void setTypeName(String typeName) {
		this.typeName = typeName;
	}

	public String getAttachFileId() {
		return attachFileId;
	}

	public void setAttachFileId(String attachFileId) {
		this.attachFileId = attachFileId;
	}

	public List<UploadFileDto> getUploadFileArr() {
		return uploadFileArr;
	}

	public void setUploadFileArr(List<UploadFileDto> uploadFileArr) {
		this.uploadFileArr = uploadFileArr;
	}

	public String getIssueOrgName() {
		return issueOrgName;
	}

	public void setIssueOrgName(String issueOrgName) {
		this.issueOrgName = issueOrgName;
	}

	public Integer getOrgType() {
		return orgType;
	}

	public void setOrgType(Integer orgType) {
		this.orgType = orgType;
	}

	public String getOrgId() {
		return orgId;
	}

	public void setOrgId(String orgId) {
		this.orgId = orgId;
	}
	
	public String getTopStatus() {
		return topStatus;
	}

	public void setTopStatus(String topStatus) {
		this.topStatus = topStatus;
	}

	@Override
	public String toString() {
		return "IssueMessageDTO [id=" + id + ", startTime=" + startTime + ", endTime=" + endTime + ", issue=" + issue
				+ ", theme=" + theme + ", content=" + content + ", issueTime=" + issueTime + ", issueUserName="
				+ issueUserName + ", issueOrgName=" + issueOrgName + ", orgType=" + orgType + ", orgId=" + orgId
				+ ", type=" + type + ", typeName=" + typeName + ", attachFileId=" + attachFileId + ", uploadFileArr="
				+ uploadFileArr + ", topStatus=" + topStatus + "]";
	}

	

}
